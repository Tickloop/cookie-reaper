{
    "name": "Cookie Reaper",
    "icons": {
        "16": "icons/cookie-reaper.png",
        "24": "icons/cookie-reaper.png",
        "48": "icons/cookie-reaper.png",
        "128": "icons/cookie-reaper.png"
    },
    "description": "Blocks annoying cookie popups by hiding them in plain sight",
    "version": "0.2",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "https://*.serverfault.com/*",
                "https://*.superuser.com/*",
                "https://*.askubuntu.com/*",
                "https://*.stackapps.com/*",
                "https://*.mathoverflow.net/*",
                "https://*.stackoverflow.com/*",
                "https://*.stackexchange.com/*"
            ],
            "js": ["cs.js"]
        }
    ]
}